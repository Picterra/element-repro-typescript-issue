{
    "compilerOptions": {
      "target": "es6",
      "strict": true,
      "module": "es6",
      "moduleResolution": "node",
      "baseUrl": "src",
      "allowJs": true,
      "sourceMap": true,
      "allowSyntheticDefaultImports": true,
      "paths": {
        "@/*": ["*"]
      }
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
